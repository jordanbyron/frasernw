- set_page_specific_title @specialization.name

.tabbable
  #react_root--template

:javascript
  window.pathways = window.pathways || {};
  window.pathways.dataForReact = #{Oj.dump(@init_data)};

.content-wrapper
  %p.admin.btn-group
    - if can? :update, @specialization
      = link_to edit_specialization_path(@specialization),
        class: "btn" do
        %i.icon-pencil
        %span= " Edit Specialty"
