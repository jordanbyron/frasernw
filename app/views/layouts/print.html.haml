!!!
%html
  %head
    :javascript
      window.pathwaysEnvironment = #{Rails.env};
    %title= page_title
    %meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
    %meta{"name"=>"viewport", :content=>"initial-scale=1.0"}

    = stylesheet_link_tag "print_information", :media => 'screen, print'
    = javascript_include_tag "jquery.1.8.3.min.js"

    = render "partials/analytics_setup"

    = csrf_meta_tag
    = yield(:head)

  %body
    = yield
